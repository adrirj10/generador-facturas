{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/generador-facturas"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "assets": [
    "src/favicon.ico",
    "src/assets",
    {
      "glob": "_redirects",
      "input": "public",
      "output": "/"
    }
  ]
}